#  Generated by Genus(TM) Synthesis Solution 17.22-s017_1
 
export WORKDIR=./test_scripts
 
irun \
   +access+rwc \
   +ncstatus \
   +nc64bit \
   +TESTFILE1=$WORKDIR/testresults/verilog/VER.FULLSCAN.counter_atpg.data.scan.ex1.ts1.verilog \
   +TESTFILE2=$WORKDIR/testresults/verilog/VER.FULLSCAN.counter_atpg.data.logic.ex1.ts2.verilog \
   +HEARTBEAT \
   +FAILSET \
   +nctimescale+1ns/1ps \
   +ncoverride_timescale \
   +ncseq_udp_delay+2ps \
   +libext+.v+.V+.z+.Z+.gz \
   +nclibdirname+$WORKDIR/Inca_libs_13_18_21 \
   -l $WORKDIR/ncverilog_FULLSCAN.log \
  -v /home/vlsi6/pdk/scl_pdk/stdlib/fs120/liberty/lib_flow_ss/tsl18fs120_scl_ss.lib \
$WORKDIR/counter.test_netlist.v \
